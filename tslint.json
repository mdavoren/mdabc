{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": [false],
        "trailing-comma": [false],
        "object-literal-sort-keys": false,
        "no-namespace": false,
        "no-conditional-assignment": false,
        "max-line-length": [true, 150],
        "no-console": [false],
        "interface-name": [false]
    },
    "rulesDirectory": []
}